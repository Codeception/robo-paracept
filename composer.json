    "license": "MIT",
    "authors": [
        {
            "name": "Davert",
            "email": "davert@codeception.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Codeception\\Task\\":"src/"
        }
    },
    "require": {
        "php": "^7.3",
        "consolidation/robo": "3.0.*",
        "ext-dom": "*",
