        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "codeception/codeception": "4.1.*",
        "squizlabs/php_codesniffer": "3.6.*",
        "overtrue/phplint": "3.0.*",
        "friendsofphp/php-cs-fixer": "3.0.*",
        "phpunit/phpunit": "9.5.*",
        "codeception/phpunit-wrapper": "^9.0.6"
    }
}
